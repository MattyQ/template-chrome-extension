{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0",
    "permissions": ["scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "<all_urls>"
    ]
}
